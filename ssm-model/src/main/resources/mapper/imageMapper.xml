<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dev.dao.ImageMapper">
	
	<insert id="savePic" parameterType="Image" useGeneratedKeys="true" keyColumn="id">
		insert into t_pic (picname,score,version,piclink) values(#{picname},#{score},#{version},#{piclink})
	</insert>


</mapper>        